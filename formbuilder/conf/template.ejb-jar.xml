<?xml version = '1.0' encoding = 'windows-1252'?>
<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN" "http://java.sun.com/dtd/ejb-jar_2_0.dtd">
<ejb-jar>
  <enterprise-beans>
    <session>
      <description>Session Bean ( Stateless )</description>
      <display-name>FormBuilder</display-name>
      <ejb-name>FormBuilder</ejb-name>
      <home>gov.nih.nci.ncicb.cadsr.formbuilder.ejb.FormBuilderHome</home>
      <remote>gov.nih.nci.ncicb.cadsr.formbuilder.ejb.FormBuilder</remote>
      <ejb-class>gov.nih.nci.ncicb.cadsr.formbuilder.ejb.impl.FormBuilderEJB</ejb-class>
      <session-type>Stateless</session-type>
      <transaction-type>Container</transaction-type>
      <env-entry>
        <description>Class name of the DAOFactory that FormBuilderEJB will use.</description>
        <env-entry-name>DAOFactoryClassName</env-entry-name>
        <env-entry-type>java.lang.String</env-entry-type>
        <env-entry-value>gov.nih.nci.ncicb.cadsr.common.persistence.dao.jdbc.JDBCDAOFactory</env-entry-value>
      </env-entry> 
      <resource-ref>      
	<res-ref-name>jdbc/@CADSR.DS.JNDI.NAME@</res-ref-name>     
	<res-type>javax.sql.DataSource</res-type>      
	<res-auth>Container</res-auth> 
       </resource-ref>       
    </session>
  </enterprise-beans>
  <assembly-descriptor>
    <container-transaction>
      <method>
        <ejb-name>FormBuilder</ejb-name>
        <method-name>*</method-name>
      </method>
      <trans-attribute>Required</trans-attribute>
    </container-transaction>
    <container-transaction>
          <method>
            <ejb-name>FormBuilder</ejb-name>
            <method-name>get*</method-name>
          </method>
          <trans-attribute>Supports</trans-attribute>
    </container-transaction>
    <container-transaction>
          <method>
            <ejb-name>FormBuilder</ejb-name>
            <method-name>validateUser</method-name>
          </method>
          <trans-attribute>Supports</trans-attribute>
    </container-transaction>    
    <container-transaction>
          <method>
            <ejb-name>FormBuilder</ejb-name>
            <method-name>retrieveCDECart</method-name>
          </method>
          <trans-attribute>Supports</trans-attribute>
    </container-transaction>  
    <container-transaction>
          <method>
            <ejb-name>FormBuilder</ejb-name>
            <method-name>retrieveFormClassifications</method-name>
          </method>
          <trans-attribute>Supports</trans-attribute>
    </container-transaction>   
    <container-transaction>
          <method>
            <ejb-name>FormBuilder</ejb-name>
            <method-name>isAllACDesignatedToContext</method-name>
          </method>
          <trans-attribute>Supports</trans-attribute>
    </container-transaction>    
    <container-transaction>
          <method>
            <ejb-name>FormBuilder</ejb-name>
            <method-name>isTargetForTriggerAction</method-name>
          </method>
          <trans-attribute>Supports</trans-attribute>
    </container-transaction>      
  </assembly-descriptor>
</ejb-jar>
