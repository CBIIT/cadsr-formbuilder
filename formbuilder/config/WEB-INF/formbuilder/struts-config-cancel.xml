<?xml version = '1.0' encoding = 'windows-1252'?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_2.dtd">
<struts-config>

  <action-mappings>
    <action name="hrefForm" path="/cancelAction" parameter="method"
      scope="request"
      type="gov.nih.nci.ncicb.cadsr.formbuilder.struts.actions.CancelAction"
      validate="false">
      <forward name="gotoModuleEdit" path="/formbuilder/moduleEdit.jsp"/>
      <forward name="gotoFormEdit" path="/formbuilder/formEdit.jsp"/>
      <forward name="gotoSearch" path="/formSearchAction.do?method=getAllForms"/>
      <forward name="gotoManageClassifications" path="/formbuilder/manageClassifications.jsp"/>
    </action>
  </action-mappings>

</struts-config>
