<?xml version = '1.0' encoding = 'windows-1252'?>
<!DOCTYPE orion-application PUBLIC "-//Evermind//DTD J2EE Application runtime 1.2//EN" "http://xmlns.oracle.com/ias/dtds/orion-application.dtd">
<orion-application>
 <user-manager class="gov.nih.nci.ncicb.cadsr.common.security.oc4j.DBUserManager">	
 	<property name="ServiceLocatorClassName" value="gov.nih.nci.ncicb.cadsr.common.servicelocator.ejb.ServiceLocatorImpl"/>
 </user-manager>
 
    	    
   <library path="security.jar" />
   <library path="common.jar" />
   <library path="spring-jdbc.jar" />
   <library path="spring.jar" />
   <library path="commons-logging.jar" />
   <library path="persistence_form.jar" />
   
  <security-role-mapping name="FORMBUILDER_USERS">
    <group name="CDE MANAGER" />
 </security-role-mapping>
 
   <data-sources path="./data-sources.xml"/>
   <principals path="./principals.xml" />
   
  <!-- use JAZN-XML by default -->
  <jazn provider="XML" location="./jazn-data.xml" />

  <log>
    <file path="application.log" />
  </log>
	<namespace-access>
		<read-access>
			<namespace-resource root="">
				<security-role-mapping name="&lt;jndi-user-role&gt;">
					<group name="CDE MANAGER" />
				</security-role-mapping>
			</namespace-resource>
		</read-access>
		<write-access>
			<namespace-resource root="">
				<security-role-mapping name="&lt;jndi-user-role&gt;">
					<group name="CDE MANAGER" />
				</security-role-mapping>
			</namespace-resource>
		</write-access>
	</namespace-access>   
</orion-application>
