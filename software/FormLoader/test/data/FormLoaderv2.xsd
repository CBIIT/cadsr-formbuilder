<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <!-- Version 1 
    see requirements/assumptions caDSR Form Loader 1.0 v2 Scope-->
  <!-- Version 2
    added minOccurs="0" to displayOrder, name and type for referenceDocument -->
  <xs:element name="forms">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="form" minOccurs="1" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="form">
    <xs:complexType mixed="true">
      <xs:sequence>
        <!-- context can be added as a default using the Form Loader -->
        <xs:element ref="context" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="createdBy" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="dateCreated" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="dateModified" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="modifiedBy" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="longName" minOccurs="1" maxOccurs="1"/>
        <xs:element ref="changeNote" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="preferredDefinition" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="publicID" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="version" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="registrationStatus" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="workflowStatusName" minOccurs="0" maxOccurs="1"/><!-- can be a default value in form loader -->
        <xs:element ref="categoryName" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="disease" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="type" minOccurs="0" maxOccurs="1"/> <!-- default in form loader -->
        <xs:element ref="designation" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="definition" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element name="headerInstruction" type="instructionType" minOccurs="0" maxOccurs="1"/>
        <xs:element name="footerInstruction" type="instructionType" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="module" minOccurs="1" maxOccurs="unbounded"/>
        <xs:element ref="protocol" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="referenceDocument" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="contactCommunication" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="contactCommunication">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="rank" minOccurs="0"/>
        <xs:element name="type" minOccurs="0">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:enumeration value="PHONE"/>
              <xs:enumeration value="EMAIL"/>
              <xs:enumeration value="FAX"/>
              <xs:enumeration value="In Person"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
        <xs:element name="value" type="xs:string" minOccurs="0"/>
        <xs:element name="organizationName" type="xs:string" minOccurs="0"/>
        <xs:element name="organizationRAI" type="xs:string" minOccurs="0"/>
        <xs:element ref="person" minOccurs="0"/>
        <xs:element ref="createdBy" minOccurs="0"/>
        <xs:element ref="dateCreated" minOccurs="0"/>
        <xs:element ref="dateModified" minOccurs="0"/>
        <xs:element ref="modifiedBy" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:simpleType name="emptyString">
    <xs:restriction base="xs:string">
      <xs:enumeration value=""/>
    </xs:restriction>
  </xs:simpleType>

  <xs:element name="rank" type="xs:string"/>

  <xs:element name="person">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="firstName" type="xs:string"/>
        <xs:element name="lastName" type="xs:string"/>
        <xs:element name="position" type="xs:string"/>
        <xs:element ref="address" minOccurs="0" maxOccurs="unbounded"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="address">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="addressLine1" type="xs:string"/>
        <xs:element name="addressLine2" type="xs:string"/>
        <xs:element name="city" type="xs:string"/>
        <xs:element name="state" type="xs:string"/>
        <xs:element name="country" type="xs:string"/>
        <xs:element name="postalCode" type="xs:string"/>
        <xs:element ref="rank" minOccurs="0"/>
        <xs:element name="type">
          <xs:simpleType>
            <xs:restriction base="xs:string">
              <xs:enumeration value="MAILING"/>
              <xs:enumeration value="Package Delivery"/>
            </xs:restriction>
          </xs:simpleType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="categoryName" type="xs:string" nillable="true"/>
  <xs:element name="disease" type="xs:string" nillable="true"/>

  <xs:element name="module">
    <xs:complexType mixed="true">
      <xs:sequence>
        <xs:element ref="displayOrder" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="maximumModuleRepeat" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="createdBy" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="dateCreated" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="dateModified" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="modifiedBy" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="longName" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="instruction" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="preferredDefinition" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="publicID" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="version" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="usageCategory" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="question" minOccurs="1" maxOccurs="unbounded"/>
        <xs:element ref="triggerAction" minOccurs="0" maxOccurs="1"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="maximumModuleRepeat" type="xs:integer" nillable="true"/>

  <xs:element name="usageCategory">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="usageType" minOccurs="0"/>
        <xs:element ref="rule" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="question">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="publicID" minOccurs="0" maxOccurs="1"/> <!-- Added V19 -->
        <xs:element ref="version" minOccurs="0" maxOccurs="1"/> <!-- Added V19 -->
        <xs:element ref="isDerived" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="displayOrder" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="createdBy" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="dateCreated" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="dateModified" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="modifiedBy" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="questionText" minOccurs="1" maxOccurs="1"/>
        <xs:element ref="instruction" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="defaultValue" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="isEditable" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="isMandatory" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="multiValue" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="dataElement" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="validValue" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="triggerAction" minOccurs="0" maxOccurs="1"/>
        <xs:element ref="questionRepetitions" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
    </xs:complexType>
  </xs:element>
  
  <xs:element name="isDerived">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="true"/>
        <xs:enumeration value="false"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="defaultValue" type="xs:string" nillable="true"/>
  <xs:element name="isEditable">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="Yes"/>
        <xs:enumeration value="No"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="isMandatory">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="Yes"/>
        <xs:enumeration value="No"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="multiValue">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="Yes"/>
        <xs:enumeration value="No"/>
      </xs:restriction>
    </xs:simpleType>
  </xs:element>
  <xs:element name="questionText" type="xs:string"/>
  <xs:element name="dataElement" type="dataElementType"/>

  <xs:complexType name="dataElementType">
    <xs:sequence>
      <xs:element ref="longName" minOccurs="0"/>
      <xs:element ref="shortName" minOccurs="0"/> 
      <xs:element ref="publicID"/>
      <xs:element ref="version"/>
      <xs:element ref="context" minOccurs="0"/>
      <xs:element ref="workflowStatusName" minOccurs="0"/>
      <xs:element ref="registrationStatus" minOccurs="0"/>
      <xs:element ref="preferredDefinition" minOccurs="0"/>
      <xs:element ref="designation" minOccurs="0" maxOccurs="unbounded"/> 
      <xs:element ref="valueDomain" minOccurs="0"/>
      <xs:element ref="dataElementDerivation" minOccurs="0"/> <!-- Added in formCartV2 -->
      <xs:element ref="referenceDocument" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="cdeBrowserLink" type="URLType" minOccurs="0" maxOccurs="1"/>
    </xs:sequence>
  </xs:complexType>

  <xs:element name="componentDataElement">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="usageCategory" minOccurs="1" maxOccurs="1"/>
        <xs:element ref="displayOrder" minOccurs="1" maxOccurs="1"/>
        <xs:element ref="dataElement" minOccurs="1" maxOccurs="1"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="preferredDefinition" type="xs:string"/>

  <xs:element name="valueDomain">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="longName"/>
        <xs:element ref="shortName"/>
        <xs:element ref="publicID"/>
        <xs:element ref="version"/>
        <xs:element name="type" type="valueDomainType"/>
        <xs:element ref="context" minOccurs="0"/>
        <xs:element ref="workflowStatusName" minOccurs="0"/>
        <xs:element ref="datatypeName" minOccurs="0"/>
        <xs:element name="decimalPlace" type="xs:string" minOccurs="0"/>
        <xs:element ref="formatName" minOccurs="0"/>
        <xs:element name="highValueNumber" type="xs:string" minOccurs="0"/>
        <xs:element name="lowValueNumber" type="xs:string" minOccurs="0"/>
        <xs:element name="maximumLengthNumber" type="xs:string" minOccurs="0"/>
        <xs:element name="minimumLengthNumber" type="xs:string" minOccurs="0"/>
        <xs:element ref="UOMName" minOccurs="0"/>
        <xs:element ref="valueDomainConcept" minOccurs="0"/> 
        <xs:element ref="permissibleValue" minOccurs="0" maxOccurs="unbounded"/> 
        <xs:element ref="referenceDocument" minOccurs="0" maxOccurs="unbounded"/>
        
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="permissibleValue"> 
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="value" minOccurs="0"/>
        <xs:element ref="valueMeaning" minOccurs="0"/>
        <xs:element name="beginDate" minOccurs="0"/>
        <xs:element name="endDate" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  
  <xs:element name="validValue">
    <xs:complexType mixed="true">
      <xs:sequence>
        <xs:element ref="displayOrder" minOccurs="0"/>
        <xs:element ref="value"/>
        <xs:element ref="meaningText"/>
        <xs:element ref="description"/>
        <xs:element ref="instruction" minOccurs="0"/>
        <xs:element ref="valueMeaning" minOccurs="0"/>
        <xs:element ref="triggerAction" minOccurs="0"/>
        <xs:element ref="createdBy" minOccurs="0"/>
        <xs:element ref="dateCreated" minOccurs="0"/>
        <xs:element ref="dateModified" minOccurs="0"/>
        <xs:element ref="modifiedBy" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  
  <xs:element name="meaningText" type="xs:string"/>
  
  <xs:element name="valueMeaning">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="publicID" minOccurs="0"/>
        <xs:element ref="version" minOccurs="0"/>
        <xs:element ref="longName" minOccurs="0"/>
        <xs:element ref="designation" minOccurs="0" maxOccurs="unbounded"/>
        <xs:element ref="definition" minOccurs="0" maxOccurs="unbounded"/> <!-- Added v19 -->
        <xs:element ref="preferredDefinition" minOccurs="0"/> <!-- Added v19 -->
        <xs:element ref="conceptDerivation" minOccurs="0" maxOccurs="unbounded"/> <!-- Added v18 -->
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  
  <xs:element name="conceptDerivation"> <!-- Added v18 -->
    <xs:complexType>
      <xs:sequence>
        <xs:element name="displayOrder"/>
          
        <xs:element name="primaryFlag"/> 
        <xs:element name="conceptCode"/> <!-- preferredName -->
        <xs:element name="conceptName"/> <!-- longName -->
        <xs:element name="integerValue"/> <!-- value -->
        <xs:element name="origin"/> <!-- origin -->
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  
  <xs:element name="valueDomainConcept">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="primaryConceptName"/>
        <xs:element name="primaryConceptCode"/>
        <xs:element name="nciTermBrowserLink" type="xs:anyURI" minOccurs="0"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>

  <xs:element name="questionRepetitions">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="repeatSequenceNumber"/>
        <xs:element name="defaultValidValue"/>
        <xs:element ref="isEditable"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>


  <xs:simpleType name="valueDomainType">
    <xs:restriction base="xs:string">
      <xs:pattern value="Enumerated|NonEnumerated"/>
    </xs:restriction>
  </xs:simpleType>


  <xs:element name="datatypeName">
    <xs:simpleType>
      <xs:restriction base="xs:string">
        <xs:enumeration value="Alpha DVG"/>
        <!-- Please use 'Date Alpha DVG' or 'Numeric Alpha DVG'	-->
        <xs:enumeration value="ALPHANUMERIC"/>
        <!-- A mix of numeric and alphabetic characters	-->
        <xs:enumeration value="binary"/>
        <!-- A representation with only two unique components.	-->
        <xs:enumeration value="BINARY OBJECT"/>
        <!--A binary large object such as a document or image.	-->
        <xs:enumeration value="BOOLEAN"/>
        <!--	The mathematical datatype associated with two-valued logic.	-->
        <xs:enumeration value="BRIDG_SET_v2.1"/>
        <!--	Distinct values in no particular order -->
        <xs:enumeration value="CHARACTER"/>
        <!-- A character string of letters, symbols, and/or numbers	-->
        <xs:enumeration value="DATE"/>
        <!--	A date specifying month, day, and year	-->
        <xs:enumeration value="Date Alpha DVG"/>
        <!--	Oracle Clinical datatype-contains alphanumeric and Date data	The Oracle "Date Alpha DVG" datatype signifies that data fields with the type Date can contain alphanumeric data in addition to the type of data defined by the original datatype. 
          The assignment of this datatype permits validation of specified alphanumeric data codes and allows them to 
          be entered without alerting the data entry operator or necessarily creating a discrepancy. For example, 
          a field used for recording a Birth Date would accept the text "Unknown" in addition to a numeric date without 
          causing an error. Examples at http://www.sscomp.com/web/training/docs/Global_Library_4_Practice.pdf -->
        <xs:enumeration value="DATE/TIME"/>
        <!--	DATE/TIME	-->
        <xs:enumeration value="DATETIME"/>
        <!--	A date (month,day,year) and time (hours,minutes,seconds).	-->
        <xs:enumeration value="Derived"/>
        <!--	Rule controlling data element content.	Registry Defined. Applied for ISO 11179 derivations -->
        <xs:enumeration value="evs.domain.Property"/>
        <!--	A local java datatype - Property.	-->
        <xs:enumeration value="evs.domain.Role"/>
        <!-- A local java datatype - Role.	-->
        <xs:enumeration value="evs.domain.Source"/>
        <!--	A local java datatype - Source.	-->
        <xs:enumeration value="evs.domain.TreeNode"/>
        <!--	A local java datatype - TreeNode.	-->
        <xs:enumeration value="HL7PPDv3.0"/>
        <!-- Parametric probability distribution.	HL7 Scheme Reference	-->
        <xs:enumeration value="ISO21090ADPartv1.0"/>
        <!-- Content model for addresses.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADv1.0"/>
        <!-- Parts for mailing and home or office addresses.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPADLv1.0"/>
        <!-- An additional locator in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPALv1.0"/>
        <!-- An address line in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPBNNv1.0"/>
        <!-- A building number numeric in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPBNRv1.0"/>
        <!-- A building number in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPBNSv1.0"/>
        <!-- A bulding number suffix in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPBRv1.0"/>
        <!--	A break in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPCARv1.0"/>
        <!-- Care of in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPCENv1.0"/>
        <!-- A census track in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPCNTv1.0"/>
        <!-- A country in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPCPAv1.0"/>
        <!-- A county or parish in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPCTYv1.0"/>
        <!-- A delivery address line in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPDALv1.0"/>
        <!-- A delivery address line in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPDIAAv1.0"/>
        <!-- A delivery installation area in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPDIQv1.0"/>
        <!-- A delivery installation qualifier in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPDIRv1.0"/>
        <!-- A direction in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPDISTv1.0"/>
        <!-- A delivery installation type in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPDMDv1.0"/>
        <!-- A delivery mode in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPDMIDv1.0"/>
        <!-- A delivery mode identifier in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPINTv1.0"/>
        <!-- An intersection in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPPOBv1.0"/>
        <!-- A post box in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPPREv1.0"/>
        <!-- A precinct in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPSALv1.0"/>
        <!-- A street address in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPSTAv1.0"/>
        <!-- A state or province in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPSTBv1.0"/>
        <!-- A street name base in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPSTRv1.0"/>
        <!-- A street name in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPSTTPv1.0"/>
        <!-- A street type in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPUNIDv1.0"/>
        <!-- A unit identifier in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPUNTv1.0"/>
        <!-- A unit designator in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPv1.0"/>
        <!-- String with a type-tag to signify its role in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ADXPZIPv1.0"/>
        <!-- A postal code in the address.	ISO 21090	-->
        <xs:enumeration value="ISO21090ANYv1.0"/>
        <!-- Local data type that represents any UML Class. -->
        <xs:enumeration value="anyClass"/>
        <!-- Defines the basic property of every data value.	ISO 21090	-->
        <xs:enumeration value="ISO21090BAGv1.0"/>
        <!-- Unordered set of values which can appear more than once.	ISO 21090	-->
        <xs:enumeration value="ISO21090BLNTNULv1.0"/>
        <!-- Constrains instance of BL not to have a nullFlavor	ISO 21090	-->
        <xs:enumeration value="ISO21090BLv1.0"/>
        <!-- Values of two-valued logic.	ISO 21090	-->
        <xs:enumeration value="ISO21090CDBasev1.0"/>
        <!-- Base concept descriptor attributes.	ISO 21090	-->
        <xs:enumeration value="ISO21090CDCVv1.0"/>
        <!-- Constrains CD to a single code to be sent for given value.	ISO 21090	-->
        <xs:enumeration value="ISO21090CDv1.0"/>
        <!-- Reference a concept defined by a referenced code system.	ISO 21090	-->
        <xs:enumeration value="ISO21090COLLv1.0"/>
        <!-- Abstract - collects common properties/behaviors of classes.	ISO 21090	-->
        <xs:enumeration value="ISO21090COv1.0"/>
        <!-- Coded values associated with a specific order.	ISO 21090	-->
        <xs:enumeration value="ISO21090CSv1.0"/>
        <!-- Coded data in simplest form, only code is not predetermined.	ISO 21090	-->
        <xs:enumeration value="ISO21090DSETv1.0"/>
        <!-- A collection containing unordered distinct/discrete values.	ISO 21090	-->
        <xs:enumeration value="ISO21090EDDCINLINv1."/>
        <!-- Constrains ED to contain only a non-referenced document.	ISO 21090	-->
        <xs:enumeration value="ISO21090EDDCREFv1.0"/>
        <!-- Constrains ED to contain only a referenced document.	ISO 21090	-->
        <xs:enumeration value="ISO21090EDIMAGEv1.0"/>
        <!-- Constrains ED so that the contents must be an image.	ISO 21090	-->
        <xs:enumeration value="ISO21090EDSIGNAT1.0"/>
        <!-- Constrains ED to only contain an XML digital signature.	ISO 21090	-->
        <xs:enumeration value="ISO21090EDSTRTITv1.0"/>
        <!-- Constrains ED to only contain a structured title.	ISO 21090	-->
        <xs:enumeration value="ISO21090EDSTRTXv1.0"/>
        <!-- Constrains ED to only contain a structured narrative.	ISO 21090	-->
        <xs:enumeration value="ISO21090EDTEXTv1.0"/>
        <!-- Constrains ED so that it may only contain plain text.	ISO 21090	-->
        <xs:enumeration value="ISO21090EDv1.0"/>
        <!-- Data only for human interpretation or machine processing.	ISO 21090	-->
        <xs:enumeration value="ISO21090EIVLv1.0"/>
        <!-- Event-related periodic interval of time.	ISO 21090	-->
        <xs:enumeration value="ISO21090ENONv1.0"/>
        <!-- Organization Name constraint on Entity Name.	ISO 21090	-->
        <xs:enumeration value="ISO21090ENPNv1.0"/>
        <!-- Person Name constraint on Entity Name.	ISO 21090	-->
        <xs:enumeration value="ISO21090ENTNv1.0"/>
        <!-- Trivial Name constraint on Entity Name.	ISO 21090	-->
        <xs:enumeration value="ISO21090ENV1.0"/>
        <!-- A name for a person, organization, place or thing.	ISO 21090	-->
        <xs:enumeration value="ISO21090ENXPv1.0"/>
        <!-- A character string token representing a part of a name.	ISO 21090	-->
        <xs:enumeration value="ISO21090GLISTv1.0"/>
        <!-- Generated sequence.	ISO 21090	-->
        <xs:enumeration value="ISO21090GTSBPIVLv1.0"/>
        <!-- Constrains QSI(TS) to intersection of IVL(TS) and PIVL(TS)	ISO 21090	-->
        <xs:enumeration value="ISO21090HISTv1.0"/>
        <!-- Items in historical order. 	ISO 21090	-->
        <xs:enumeration value="ISO21090HXITv1.0"/>
