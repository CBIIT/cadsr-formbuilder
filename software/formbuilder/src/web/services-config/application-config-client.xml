<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
						http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.1.xsd">

	<!-- Import the files for each remote service -->
	<import resource="CADSR_application-config-client.xml"/>
	<import resource="OC_application-config-client.xml"/>
	
	<!-- Make an entry for ServiceInfo for each remote service. The default service bean must be called "ServiceInfo" -->
	<alias alias="ServiceInfo" name="CaDsrServiceInfo"/>
	
</beans>