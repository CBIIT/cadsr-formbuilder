    <processgroup name="DataElementConceptDetails" class="oracle.cle.process.DisplayGroup" end="true" initial="DataElementConceptDetailsRouter">
    <process name="GetDataElementConceptDetails" class="gov.nih.nci.ncicb.cadsr.common.process.GetDataElementConceptDetails" end="false">
    </process> 
    <process name="DataElementConceptDetailsPage" class="oracle.clex.process.CreateJSPPage" end="true">
        <argument type="String" value="&docpath;dataElementConceptDetails.jsp"/>
    </process>
    <process name="FailurePage" class="oracle.clex.process.CreateJSPPage" end="true">
        <argument type="String" value="&docpath;FailurePage.jsp"/>
    </process>
    <process name="DataElementConceptDetailsRouter" class="oracle.clex.process.ProcessRouter" end="false">
        <argument type="Integer" value="1"/>
        <argument type="Boolean" value="true"/>
        <argument type="String" value="success"/>
        <infoalias local="pp1" global="dataElementConceptDetails"/>
    </process>
<transition src="GetDataElementConceptDetails" condition="success" dest="DataElementConceptDetailsPage"/>
<transition src="GetDataElementConceptDetails" condition="failure" dest="FailurePage"/>
<transition src="DataElementConceptDetailsRouter" condition="tc1" dest="GetDataElementConceptDetails"/>
