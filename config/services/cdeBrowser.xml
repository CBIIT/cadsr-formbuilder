<transition src="SearchRouter" condition="success" dest="DataElementsGroup"/>
<transition src="SearchRouter" condition="tc1" dest="DataElementsGroup"/>
<transition src="SearchRouter" condition="tc2" dest="DataElementsGroup"/>
<transition src="SearchRouter" condition="failure" dest="DisplayFailureGroup"/>
<transition src="SearchRouter" condition="tc3" dest="ValidValuesGroup"/>
<transition src="SearchRouter" condition="tc4" dest="GetDetailsGroup"/>
<transition src="SearchRouter" condition="tc5" dest="ClassificationSchemesGroup"/>
<transition src="SearchRouter" condition="tc6" dest="ProtocolsGroup"/>
<transition src="SearchRouter" condition="tc7" dest="ListOfValuesGroup"/>
<transition src="SearchRouter" condition="tc8" dest="FileDownloadGroup"/>
<transition src="SearchRouter" condition="tc9" dest="FileDownloadGroup"/>
<transition src="SearchRouter" condition="tc10" dest="FileDownloadGroup"/>
<transition src="SearchRouter" condition="tc11" dest="ListOfValuesGroup"/>
<transition src="SearchRouter" condition="tc12" dest="ListOfValuesGroup"/>
<transition src="SearchRouter" condition="tc13" dest="DataElementConceptDetails"/>
<transition src="SearchRouter" condition="tc14" dest="DataElementDerivationGroup"/>
<grouptransition srcgroup="DataElementsGroup" srcprocess="DataElementsPage" dest="SearchRouter"/>
<grouptransition srcgroup="DataElementsGroup" srcprocess="FailurePage" dest="SearchRouter"/>
<grouptransition srcgroup="DisplayFailureGroup" srcprocess="FailurePage" dest="SearchRouter"/>
<grouptransition srcgroup="ValidValuesGroup" srcprocess="FailurePage" dest="SearchRouter"/>
<grouptransition srcgroup="ValidValuesGroup" srcprocess="ListValidValuesPage" dest="SearchRouter"/>
<grouptransition srcgroup="GetDetailsGroup" srcprocess="DataElementDetailsPage" dest="SearchRouter"/>
<grouptransition srcgroup="GetDetailsGroup" srcprocess="FailurePage" dest="SearchRouter"/>
<grouptransition srcgroup="ClassificationSchemesGroup" srcprocess="FailurePage" dest="SearchRouter"/>
<grouptransition srcgroup="ClassificationSchemesGroup" srcprocess="ClassificationSchemesListPage" dest="SearchRouter"/>
<grouptransition srcgroup="FileDownloadGroup" srcprocess="ExcelFile" dest="SearchRouter"/>
<grouptransition srcgroup="FileDownloadGroup" srcprocess="XMLFile" dest="SearchRouter"/>
<grouptransition srcgroup="FileDownloadGroup" srcprocess="Template" dest="SearchRouter"/>
