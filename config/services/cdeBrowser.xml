     <process name="GetClassificationSchemes" class="gov.nih.nci.ncicb.cadsr.common.process.GetClassificationSchemes" end="false">
    </process>  
    <process name="ClassificationSchemesListPage" class="oracle.clex.process.CreateJSPPage" end="true">
        <argument type="String" value="&docpath;classifications.jsp"/>
    </process>
<transition src="ClassificationSchemesRouter" condition="tc1" dest="GetClassificationSchemes"/>
<transition src="GetClassificationSchemes" condition="success" dest="ClassificationSchemesListPage"/>
<transition src="GetClassificationSchemes" condition="failure" dest="FailurePage"/>
    </processgroup>
    <processgroup name="FileDownloadGroup" class="oracle.cle.process.DisplayGroup" end="true" initial="FileDownloadRouter">
    <process name="FileDownloadRouter" class="oracle.clex.process.ProcessRouter" end="false">
        <argument type="Integer" value="3"/>
        <argument type="Boolean" value="true"/>
        <argument type="String" value="success"/>
        <infoalias local="pp1" global="excelDownload"/>
        <infoalias local="pp2" global="xmlDownload"/>
        <infoalias local="pp3" global="viewTemplate"/>
    </process>
