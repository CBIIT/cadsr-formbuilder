<processgroup name="DataElementDerivationGroup" class="oracle.cle.process.DisplayGroup" end="true" initial="DataElementDerivationRouter">
     <process name="GetDataElementDerivation" class="gov.nih.nci.ncicb.cadsr.common.process.GetDataElementDerivation" end="false">
    </process> 
    <process name="DataElementDerivationPage" class="oracle.clex.process.CreateJSPPage" end="true">
        <argument type="String" value="&docpath;dataElementDerivation.jsp"/>
