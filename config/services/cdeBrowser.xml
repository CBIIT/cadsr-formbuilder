<!DOCTYPE service SYSTEM ""[
<!ENTITY docpath "/cdebrowser/">
]>
<service name="FormBuilder" class="oracle.cle.process.Service" initial="SearchRouter">
    <process name="SearchRouter" class="oracle.clex.process.ProcessRouter" end="false">
        <argument type="Integer" value="14"/>
        <argument type="Boolean" value="false"/>
        <argument type="String" value="success"/>
        <infoalias local="pp1" global="searchDataElements"/>
        <infoalias local="pp2" global="removeDeParameters"/>
        <infoalias local="pp3" global="listValidValuesForDataElements"/>
        <infoalias local="pp4" global="dataElementDetails"/>
        <infoalias local="pp5" global="classificationsForDataElements"/>
        <infoalias local="pp6" global="protocolsForDataElements"/>
        <infoalias local="pp7" global="classificationsLOV"/>
        <infoalias local="pp8" global="excelDownload"/>
        <infoalias local="pp9" global="xmlDownload"/>
        <infoalias local="pp10" global="viewTemplate"/>
        <infoalias local="pp11" global="valueDomainsLOV"/>
        <infoalias local="pp12" global="dataElementConceptsLOV"/>
        <infoalias local="pp13" global="dataElementConceptDetails"/>
        <infoalias local="pp14" global="dataElementDerivations"/>    
    </process>
    <processgroup name="DataElementsGroup" class="oracle.cle.process.DisplayGroup" end="true" initial="DataElementsRouter">
    <process name="ResetParameters" class="gov.nih.nci.ncicb.cadsr.common.process.ResetParameters" end="false">
    </process>  
    <process name="DataElementsRouter" class="oracle.clex.process.ProcessRouter" end="false">
        <argument type="Integer" value="2"/>
        <argument type="Boolean" value="true"/>
        <argument type="String" value="success"/>
        <infoalias local="pp1" global="searchDataElements"/>
        <infoalias local="pp2" global="resetDeParameters"/>
    </process>
     <process name="GetDataElements" class="gov.nih.nci.ncicb.cadsr.common.process.GetDataElements" end="false">
    </process>   
    <process name="DataElementsPage" class="oracle.clex.process.CreateJSPPage" end="true">
        <argument type="String" value="&docpath;dataElementsSearch.jsp"/>
    </process>
    <process name="FailurePage" class="oracle.clex.process.CreateJSPPage" end="true">
        <argument type="String" value="&docpath;FailurePage.jsp"/>
    </process>
<transition src="ResetParameters" condition="failure" dest="FailurePage"/>
<transition src="ResetParameters" condition="success" dest="DataElementsPage"/>
<transition src="DataElementsRouter" condition="success" dest="GetDataElements"/>
<transition src="DataElementsRouter" condition="tc2" dest="ResetParameters"/>
<transition src="DataElementsRouter" condition="tc1" dest="GetDataElements"/>
<transition src="GetDataElements" condition="success" dest="DataElementsPage"/>
<transition src="GetDataElements" condition="failure" dest="FailurePage"/>
    </processgroup>
    <processgroup name="DisplayFailureGroup" class="oracle.cle.process.DisplayGroup" end="true" initial="FailurePageRouter">
    <process name="FailurePageRouter" class="oracle.clex.process.ProcessRouter" end="false">
        <argument type="Integer" value="2"/>
        <argument type="Boolean" value="false"/>
        <argument type="String" value="success"/>
        <infoalias local="pp1" global="failure"/>
    </process>
    <process name="FailurePage" class="oracle.clex.process.CreateJSPPage" end="true">
        <argument type="String" value="&docpath;failurePage.jsp"/>
    </process>
<transition src="FailurePageRouter" condition="success" dest="FailurePage"/>
<transition src="FailurePageRouter" condition="failure" dest="FailurePage"/>
    </processgroup>
    <processgroup name="ValidValuesGroup" class="oracle.cle.process.DisplayGroup" end="true" initial="ValidValuesRouter">
    <process name="ValidValuesRouter" class="oracle.clex.process.ProcessRouter" end="false">
        <argument type="Integer" value="1"/>
        <argument type="Boolean" value="true"/>
        <argument type="String" value="success"/>
        <infoalias local="pp1" global="listValidValuesForDataElements"/>
    </process>
    <process name="FailurePage" class="oracle.clex.process.CreateJSPPage" end="true">
        <argument type="String" value="&docpath;FailurePage.jsp"/>
    </process>
     <process name="GetValidValues" class="gov.nih.nci.ncicb.cadsr.common.process.GetValidValues" end="false">
    </process>  
    <process name="ListValidValuesPage" class="oracle.clex.process.CreateJSPPage" end="true">
        <argument type="String" value="&docpath;validValuesList.jsp"/>
    </process>
<transition src="ValidValuesRouter" condition="tc1" dest="GetValidValues"/>
<transition src="ValidValuesRouter" condition="success" dest="GetValidValues"/>
<transition src="GetValidValues" condition="success" dest="ListValidValuesPage"/>
<transition src="GetValidValues" condition="failure" dest="FailurePage"/>
    </processgroup>
    <processgroup name="GetDetailsGroup" class="oracle.cle.process.DisplayGroup" end="true" initial="GetDetailsRouter">
     <process name="GetDataElementDetails" class="gov.nih.nci.ncicb.cadsr.common.process.GetDataElementDetails" end="false">
    </process> 
    <process name="DataElementDetailsPage" class="oracle.clex.process.CreateJSPPage" end="true">
        <argument type="String" value="&docpath;dataElementDetails.jsp"/>
    </process>
    <process name="GetDetailsRouter" class="oracle.clex.process.ProcessRouter" end="false">
        <argument type="Integer" value="1"/>
        <argument type="Boolean" value="true"/>
        <argument type="String" value="success"/>
