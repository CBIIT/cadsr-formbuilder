    </process>
    <process name="DataElementConceptsLovPage" class="oracle.clex.process.CreateJSPPage" end="true">
        <argument type="String" value="&docpath;dataElementConceptsLOV.jsp"/>
    </process>
<transition src="GetLovRouter" condition="tc1" dest="GetClassificationsLov"/>
<transition src="GetLovRouter" condition="tc2" dest="GetValueDomainsLOV"/>
<transition src="GetLovRouter" condition="tc3" dest="GetDataElementConceptsLOV"/>
<transition src="GetClassificationsLov" condition="failure" dest="FailurePage"/>
<transition src="GetClassificationsLov" condition="success" dest="ClassificationsLovPage"/>
<transition src="GetValueDomainsLOV" condition="success" dest="ValueDomainsLovPage"/>
<transition src="GetValueDomainsLOV" condition="failure" dest="FailurePage"/>
<transition src="GetDataElementConceptsLOV" condition="success" dest="DataElementConceptsLovPage"/>
<transition src="GetDataElementConceptsLOV" condition="failure" dest="FailurePage"/>
    </processgroup>
    <processgroup name="ProtocolsGroup" class="oracle.cle.process.DisplayGroup" end="true" initial="ProtocolsRouter">
    <process name="ProtocolsRouter" class="oracle.clex.process.ProcessRouter" end="false">
        <argument type="Integer" value="1"/>
        <argument type="Boolean" value="true"/>
        <argument type="String" value="success"/>
        <infoalias local="pp1" global="protocolsForDataElements"/>
    </process>
  <process name="GetProtocols" class="gov.nih.nci.ncicb.cadsr.common.process.GetProtocols" end="false">
    </process>  
    <process name="FailurePage" class="oracle.clex.process.CreateJSPPage" end="true">
        <argument type="String" value="&docpath;FailurePage.jsp"/>
    </process>
    <process name="ProtocolsListPage" class="oracle.clex.process.CreateJSPPage" end="true">
        <argument type="String" value="&docpath;protocolsList.jsp"/>
