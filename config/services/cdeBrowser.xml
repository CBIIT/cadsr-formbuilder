    <process name="XMLDownload" class="gov.nih.nci.ncicb.cadsr.common.process.GetXMLDownload" end="false">
    </process>
    <process name="ViewTemplate" class="gov.nih.nci.ncicb.cadsr.common.process.ViewTemplate" end="false">
    </process>
    <process name="ExcelFile" class="gov.nih.nci.ncicb.cadsr.common.process.DownloadExcel" end="true">
    </process>
    <process name="XMLFile" class="gov.nih.nci.ncicb.cadsr.common.process.DownloadXML" end="true">
    </process>
    <process name="Template" class="gov.nih.nci.ncicb.cadsr.common.process.DownloadTemplate" end="true">
    </process> 
    <process name="FailurePage" class="oracle.clex.process.CreateJSPPage" end="true">
        <argument type="String" value="&docpath;FailurePage.jsp"/>
    </process>
<transition src="FileDownloadRouter" condition="tc1" dest="ExcelDownload"/>
<transition src="FileDownloadRouter" condition="tc2" dest="XMLDownload"/>
<transition src="FileDownloadRouter" condition="tc3" dest="ViewTemplate"/>
<transition src="FileDownloadRouter" condition="success" dest="ExcelDownload"/>
<transition src="ExcelDownload" condition="success" dest="ExcelFile"/>
<transition src="ExcelDownload" condition="failure" dest="FailurePage"/>
<transition src="XMLDownload" condition="success" dest="XMLFile"/>
<transition src="XMLDownload" condition="failure" dest="FailurePage"/>
<transition src="ViewTemplate" condition="success" dest="Template"/>
<transition src="ViewTemplate" condition="failure" dest="FailurePage"/>
    </processgroup>
    <processgroup name="ListOfValuesGroup" class="oracle.cle.process.DisplayGroup" end="true" initial="GetLovRouter">
    <process name="GetLovRouter" class="oracle.clex.process.ProcessRouter" end="false">
        <argument type="Integer" value="3"/>
        <argument type="Boolean" value="true"/>
        <argument type="String" value="success"/>
