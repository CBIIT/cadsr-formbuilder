    </process>
    <process name="FailurePage" class="oracle.clex.process.CreateJSPPage" end="true">
        <argument type="String" value="&docpath;FailurePage.jsp"/>
    </process>
<transition src="GetDataElementDetails" condition="success" dest="DataElementDetailsPage"/>
<transition src="GetDataElementDetails" condition="failure" dest="FailurePage"/>
<transition src="GetDetailsRouter" condition="tc1" dest="GetDataElementDetails"/>
<transition src="GetDetailsRouter" condition="success" dest="GetDataElementDetails"/>
    </processgroup>
    <processgroup name="ClassificationSchemesGroup" class="oracle.cle.process.DisplayGroup" end="true" initial="ClassificationSchemesRouter">
    <process name="ClassificationSchemesRouter" class="oracle.clex.process.ProcessRouter" end="false">
        <argument type="Integer" value="1"/>
        <argument type="Boolean" value="true"/>
        <argument type="String" value="success"/>
