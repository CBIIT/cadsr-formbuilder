<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>
  <bean id="daoFactory" class="gov.nih.nci.ncicb.cadsr.dao.impl.EDCIDAOFactoryImpl"/>
  <bean id="domainObjectFactory" class="gov.nih.nci.ncicb.cadsr.dao.DomainObjectFactory"  singleton="false"/>
  <bean id="queryMetadataService" class="gov.nih.nci.ncicb.cadsr.serviceimpl.QueryMetadataServiceImpl" singleton="false" >
     <property name="daoFactory"><ref local="daoFactory" /></property>
  </bean>
  <bean id="caAdapterService" class="gov.nih.nci.ncicb.cadsr.serviceimpl.CaAdapterServiceImpl" singleton="false" >
     <property name="csvEDCIGL7MappingFile"><value>Pathofthemappingfile</value></property>
  </bean>
 
  <bean id="appService" class="gov.nih.nci.system.applicationservice.ApplicationServiceProvider" 
              factory-method="getRemoteInstance">
        <constructor-arg><value>http://cbiodev104.nci.nih.gov:59080/cacore32/http/remoteService</value></constructor-arg>
  </bean>
  <bean id="dataSource" class="oracle.jdbc.pool.OracleDataSource">
     <property name="URL"><value>jdbc:oracle:thin:@cbiodb2-d.nci.nih.gov:1521:CBDEV</value></property>
     <property name="user"><value>sbrext</value></property>
     <property name="password"><value>jjuser</value></property>    
  </bean>
  <bean id="txManager" class="org.springframework.jdbc.datasource.DataSourceTransactionManager">
     <property name="dataSource" ref="dataSource"/>
  </bean>  
  <bean id="generateMessageServiceTarget" class="gov.nih.nci.ncicb.cadsr.serviceimpl.GenerateMessageServiceImpl" singleton="false" >
     <property name="queryMetadataService" ><ref local="queryMetadataService"/></property>
     <property name="caAdapterService" ><ref local="caAdapterService"/></property>
     <property name="daoFactory"><ref local="daoFactory" /></property>     
  </bean>  
  <bean id="generateMessageService" class="org.springframework.transaction.interceptor.TransactionProxyFactoryBean" >
      <property name="transactionManager" ref="txManager"/>
      <property name="target" ref="generateMessageServiceTarget"/>
      <property name="transactionAttributeSource">
          <bean class="org.springframework.transaction.annotation.AnnotationTransactionAttributeSource"/>
      </property>
  </bean>  
  <bean id="globalDefinitionsDAO" class="gov.nih.nci.ncicb.cadsr.dao.impl.GlobalDefinitionsDAOImpl" >
     <property name="dataSource"><ref local="dataSource"></ref></property>
     <property name="appService"><ref local="appService"></ref></property>
     <property name="domainObjectFactory"><ref local="domainObjectFactory"></ref></property>
  </bean>
  <!--bean id="globalDefinitionsDAO" class="org.springframework.transaction.interceptor.TransactionProxyFactoryBean" >
      <property name="transactionManager" ref="txManager"/>
      <property name="target" ref="globalDefinitionsDAOTarget"/>
      <property name="transactionAttributeSource">
          <bean class="org.springframework.transaction.annotation.AnnotationTransactionAttributeSource"/>
      </property>
  </bean-->  
  <bean id="instrumentDAO" class="gov.nih.nci.ncicb.cadsr.dao.impl.InstrumentDAOImpl" >
     <property name="dataSource"><ref local="dataSource"></ref></property>
     <property name="appService"><ref local="appService"></ref></property>
     <property name="domainObjectFactory"><ref local="domainObjectFactory"></ref></property>     
  </bean>
  <!--bean id="instrumentDAO" class="org.springframework.transaction.interceptor.TransactionProxyFactoryBean" >
      <property name="transactionManager" ref="txManager"/>
      <property name="target" ref="instrumentDAOTarget"/>
      <property name="transactionAttributeSource">
          <bean class="org.springframework.transaction.annotation.AnnotationTransactionAttributeSource"/>
      </property>
  </bean-->  
  <bean id="dataElement" class="gov.nih.nci.ncicb.cadsr.edci.domain.impl.DataElementImpl" singleton="false" />
  <bean id="globalDefinitions" class="gov.nih.nci.ncicb.cadsr.edci.domain.impl.GlobalDefinitionsImpl" singleton="false"/>
  <bean id="dataElementGroup" class="gov.nih.nci.ncicb.cadsr.edci.domain.impl.DataElementGroupImpl" singleton="false" />
  <bean id="dataElementConcept" class="gov.nih.nci.ncicb.cadsr.edci.domain.impl.DataElementConceptImpl" singleton="false" />
  <bean id="dataElementText" class="gov.nih.nci.ncicb.cadsr.edci.domain.impl.DataElementTextImpl" singleton="false" />
  <bean id="valueDomain" class="gov.nih.nci.ncicb.cadsr.edci.domain.impl.ValueDomainImpl" singleton="false" />
  <bean id="eVDSubset" class="gov.nih.nci.ncicb.cadsr.edci.domain.impl.EVDSubsetImpl" singleton="false" />
  <bean id="eVDElement" class="gov.nih.nci.ncicb.cadsr.edci.domain.impl.EVDElementImpl" singleton="false" />
  <bean id="eVDElementText" class="gov.nih.nci.ncicb.cadsr.edci.domain.impl.EVDElementTextImpl" singleton="false" />  
  <bean id="elementInSubset" class="gov.nih.nci.ncicb.cadsr.edci.domain.impl.ElementInSubsetImpl" singleton="false" />    
  <bean id="alternateDesignation" class="gov.nih.nci.ncicb.cadsr.edci.domain.impl.AlternateDesignationImpl" singleton="false" />    
  <bean id="itemDef" class="gov.nih.nci.ncicb.cadsr.edci.domain.impl.ItemDefImpl"  singleton="false" />
  <bean id="groupDef" class="gov.nih.nci.ncicb.cadsr.edci.domain.impl.GroupDefImpl" singleton="false" />
