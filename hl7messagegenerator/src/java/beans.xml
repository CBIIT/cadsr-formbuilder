  <bean id="groupRef" class="gov.nih.nci.ncicb.cadsr.edci.domain.impl.GroupRefImpl" singleton="false" />
  <bean id="sectionRef" class="gov.nih.nci.ncicb.cadsr.edci.domain.impl.SectionRefImpl" singleton="false" />
  <bean id="groupAlias" class="gov.nih.nci.ncicb.cadsr.edci.domain.impl.GroupAliasImpl" singleton="false" />
  <bean id="explicitItemRefRepetition" class="gov.nih.nci.ncicb.cadsr.edci.domain.impl.ExplicitItemRefRepetitionImpl" singleton="false" />
  <bean id="triggeredAction" class="gov.nih.nci.ncicb.cadsr.edci.domain.impl.TriggeredActionImpl" singleton="false" />
  <bean id="instruction" class="gov.nih.nci.ncicb.cadsr.edci.domain.impl.InstructionImpl" singleton="false" />
